{
  "name": "vitest-vs-rstest-benchmark",
  "version": "1.0.0",
  "description": "Benchmark react component testing between Vitest and Rspack's RSTest runner.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test:vitest": "vitest run",
    "test:rstest": "rstest run -c rstest.config.ts",
    "bench": "tsx scripts/benchmark.ts",
    "bench:readme": "npm run bench:matrix",
    "bench:ag-grid": "tsx scripts/ag-grid-benchmark.ts",
    "bench:ag-grid-dom": "tsx scripts/ag-grid-dom-benchmark.ts",
    "bench:ag-grid-count": "tsx scripts/ag-grid-count-benchmark.ts",
    "bench:matrix": "tsx scripts/matrix-benchmark.ts",
    "test": "npm run test:vitest && npm run test:rstest"
  },
  "dependencies": {
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.5.2",
    "ag-grid-community": "32.3.3",
    "ag-grid-react": "32.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@rstest/core": "^0.6.8",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "5.1.1",
    "happy-dom": "^20.0.11",
    "jsdom": "25.0.1",
    "tsx": "^4.20.6",
    "typescript": "5.6.3",
    "vite": "5.4.9",
    "vitest": "2.1.4"
  }
}
